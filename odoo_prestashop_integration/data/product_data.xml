<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <!--Create a prestashop discount product-->
        <record id="discount_product" model="product.product">
            <field name="name">Prestashop Discount Product</field>
            <field name="detailed_type">service</field>
            <field name="default_code">prestashop_discount_product</field>
        </record>

        <!--Create a prestashop shipping product-->
        <record id="shipping_product" model="product.product">
            <field name="name">Prestashop Shipping Product</field>
            <field name="detailed_type">service</field>
            <field name="default_code">prestashop_shipping_product</field>
        </record>

        <!--Create a prestashop gift card product-->
        <record id="gift_card_product" model="product.product">
            <field name="name">prestashop Gift Card Product</field>
            <field name="detailed_type">service</field>
            <field name="default_code">prestashop_gift_card_product</field>
        </record>
    </data>
</odoo>
